        url('../fonts/icomoon.woff?mxml7f') format('woff'),
        url('../fonts/icomoon.svg?mxml7f#icomoon') format('svg');
    font-weight: normal;
    font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
